{
  "extends": "airbnb",
  "globals": {
    "NylasEnv": false,
    "$n": false
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "react/prop-types": [2, {"ignore": ["children"]}],
    "eqeqeq": [2, "smart"],
    "id-length": [0],
    "no-loop-func": [0],
    "new-cap": [2, {"capIsNew": false}]
  }
}
